!!!
%html{:lang => "en"}
  %head
    %meta{:charset => "utf-8"}
      %meta{:content => "IE=Edge,chrome=1", "http-equiv" => "X-UA-Compatible"}
        %meta{:content => "width=device-width, initial-scale=1.0", :name => "viewport"}
          %title= content_for?(:title) ? yield(:title) : "MyClass102" 
          = csrf_meta_tags
          = stylesheet_link_tag "application", :media => "all"
          = favicon_link_tag 'apple-touch-icon-144x144-precomposed.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '144x144'
          = favicon_link_tag 'apple-touch-icon-114x114-precomposed.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '114x114'
          = favicon_link_tag 'apple-touch-icon-72x72-precomposed.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png', :sizes => '72x72'
          = favicon_link_tag 'apple-touch-icon-precomposed.png', :rel => 'apple-touch-icon-precomposed', :type => 'image/png'
          = favicon_link_tag 'favicon.ico', :rel => 'shortcut icon'
          = javascript_include_tag "application"
  %body
    .navbar.navbar-default.navbar-static-top
      .container
        %button.navbar-toggle{"data-target" => ".navbar-responsive-collapse", "data-toggle" => "collapse", :type => "button"}
          %span.icon-bar
          %span.icon-bar
          %span.icon-bar
        /%a.navbar-brand{:href => "/"}
        /MyClass102:
        /= @tenant.name
        .navbar-collapse.collapse.navbar-responsive-collapse
          %ul.nav.navbar-nav
            - if current_user
              = link_to "<i class='fa fa-dashboard'></i> Dashboard".html_safe, root_path, class: 'btn btn-primary sign-up'
              = link_to "<i class='fa fa-users'></i> Clients".html_safe, clients_path, class: 'btn btn-primary sign-up'
              = link_to "<i class='fa fa-gavel'></i> Courses".html_safe, courses_path, class: 'btn btn-primary sign-up'
              = link_to "<i class='fa fa-users'></i> Members".html_safe, members_path, class: 'btn btn-primary sign-up'
              = link_to "<i class='fa fa-download'></i> Income".html_safe, root_path, class: 'btn btn-warning sign-up'
              = link_to "<i class='fa fa-upload'></i> Expence".html_safe, root_path, class: 'btn btn-warning sign-up'
              = link_to "<i class='fa fa-flash'></i> Tasks".html_safe, root_path, class: 'btn btn-warning sign-up'
              = link_to "<i class='fa fa-exclamation-triangle'></i> Errors".html_safe, root_path, class: 'btn btn-warning sign-up'
              = link_to current_user.username, edit_user_registration_path, class: 'btn btn-danger sign-up'
              = link_to "<i class='fa fa-sign-out'></i> Logout".html_safe, destroy_user_session_path, :method => :delete, class: 'btn btn-danger sign-up'
            - else
              %h4 Myclass102
              /= link_to 'Sign In', new_user_session_path, class: 'btn btn-primary'
    .container
      .row
        .col-lg-12
          = bootstrap_flash
          = yield
          = rjv_tags
      %footer.navbar-fixed-bottom.row-fluid.container.navbar-inner
        %ul.nav.navbar-nav
          - if current_user
            = link_to "<i class='fa fa-shopping-cart'></i> Enrollment rep".html_safe, enrollments_path, class: 'btn btn-primary sign-up'
            = link_to "<i class='fa fa-line-chart'></i> Attend rep".html_safe, attendances_path, class: 'btn btn-primary sign-up'
            = link_to "<i class='fa fa-bar-chart'></i> Events rep".html_safe, events_path, class: 'btn btn-primary sign-up'
            = link_to "<i class='fa fa-gears'></i> AttendanceRates".html_safe, attendance_rates_path, class: 'btn btn-primary sign-up'
            = link_to "<i class='fa fa-wrench'></i> Attr1s".html_safe, attr1s_path, class: 'btn btn-primary sign-up'
            = link_to "<i class='fa fa-sliders'></i> Attr2s".html_safe, attr2s_path, class: 'btn btn-primary sign-up'
            = link_to "<i class='fa fa-sliders'></i> Attr3s".html_safe, attr3s_path, class: 'btn btn-primary sign-up'
            = link_to "<i class='fa fa-bank'></i> Offices".html_safe, offices_path, class: 'btn btn-primary sign-up'
            = link_to "<i class='fa fa-bank'></i> Rooms".html_safe, rooms_path, class: 'btn btn-primary sign-up'
