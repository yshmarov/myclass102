- model_class = Enrollment
.page-header
  %h1=t '.title', :default => model_class.model_name.human.titleize

%p
  %strong= model_class.human_attribute_name(:course_id) + ':'
  %br
  = @enrollment.course_id
  = @enrollmenttotalduepaymentclient
  /= @enrollmentenrollmenttotalduepaymentclientz
%p
  %strong= model_class.human_attribute_name(:client_id) + ':'
  %br
  = @enrollment.client_id
%p
  %strong= model_class.human_attribute_name(:tenant_id) + ':'
  %br
  = @enrollment.tenant_id
%p
  %strong= model_class.human_attribute_name(:member_id) + ':'
  %br
  = @enrollment.member_id
%p
  %strong= model_class.human_attribute_name(:coupon_id) + ':'
  %br
  = @enrollment.coupon_id


= link_to t('.back', :default => t("helpers.links.back")), enrollments_path, :class => 'btn btn-default'
= link_to t('.edit', :default => t("helpers.links.edit")), edit_enrollment_path(@enrollment), :class => 'btn btn-default'
= link_to t('.destroy', :default => t("helpers.links.destroy")), enrollment_path(@enrollment), :method => "delete", :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) }, :class => 'btn btn-danger'
